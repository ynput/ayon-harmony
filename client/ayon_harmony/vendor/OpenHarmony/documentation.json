{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": ["."],
        "includePattern": ".+\\.js(doc|x)?$",
        "exclude": [ "./openHarmony_tools.js" ]
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "encoding": "utf8",               
        "destination": "./docs/",          
        "recurse": true
    }
}